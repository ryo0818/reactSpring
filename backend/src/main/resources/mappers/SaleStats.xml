<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.repository.StatsTableRepository">

  <!-- 会社コード全取得 -->
  <select id="getStatsList" parameterType="string"
          resultType="com.example.demo.entity.StatusEntity">
    SELECT
      status            AS status,      
      statusname        AS statusName,  
      mycompanycode
    FROM status
    WHERE mycompanycode = #{mycompanycode}
  </select>

</mapper>
